{
  "eslint.validate": [
    {
      "language": "go",
      "autoFix": true
    }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "sqlite3-extraction",
      "database": "${workspaceFolder:devops-ic-collector}/database/sqlite/repository-dataset.db"
    }
  ],
  "sqltools.useNodeRuntime": true
}
